{
  "name": "clinic-queue-system",
  "version": "1.0.0",
  "description": "Smart digital queue solution for African healthcare clinics",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node database/migrate.js",
    "db:create": "mysql -u root -p < database/schema.sql",
    "db:seed": "mysql -u root -p clinic_queue < database/seeds.sql",
    "db:reset": "npm run db:create && npm run db:seed"
  },
  "keywords": [
    "queue",
    "clinic",
    "healthcare",
    "africa",
    "digital",
    "management"
  ],
  "author": "Hackathon Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.0",
    "rate-limiter-flexible": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.3"
  }
}
